<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <system.diagnostics>
    <trace autoflush="true" indentsize="4">
      <listeners>        
        <add name="configConsoleListener" 
             type="System.Diagnostics.TextWriterTraceListener"
            initializeData="TextWriterOutput.log" />
      </listeners>
    </trace>
  </system.diagnostics>
</configuration>